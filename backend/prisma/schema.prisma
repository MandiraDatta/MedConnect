

datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}


model Doctorlogin {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())

  doctor Doctor?
}

model Doctor {
  id           Int        @id @default(autoincrement())
  email        String     @unique
  name         String?
  profileImage String?
  createdAt    DateTime   @default(now())

  doctorloginId Int?      @unique
  doctorlogin   Doctorlogin? @relation(fields: [doctorloginId], references: [id])
}


model User {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  name       String?
  supabaseId String?  @unique
  password   String?
  createdAt  DateTime @default(now())
}
