generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
}

model Doctorlogin {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Doctor {
  id           Int      @id @default(autoincrement())
  email        String   @unique
  name         String?
  profileImage String?
  createdAt    DateTime @default(now())
}
