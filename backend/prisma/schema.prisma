

datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}


model Doctor {
  id           Int        @id @default(autoincrement())
  email        String     @unique
  name         String?
  profileImage String?
  createdAt    DateTime   @default(now())
}


model DoctorLogin {
  id           Int       @id @default(autoincrement())
  email        String    @unique
  name         String?
  supabaseId   String?   @unique
  password     String?
  profileImage String?
  phone        String?
  lastLoginAt  DateTime?
  createdAt    DateTime  @default(now())
  updatedAt    DateTime  @updatedAt
}
